{
  "manifest_version": 2,

  "name": "Watch Something",
  "description": "Picks something for you to watch, so you don't have to",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
